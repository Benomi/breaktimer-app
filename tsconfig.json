{
  "compilerOptions": {
    "typeRoots": ["app/types"],
    "target": "es2019",
    "baseUrl": "./",
    "rootDir": "./",
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "pretty": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "lib": ["es2020", "dom"],
    "esModuleInterop": true,
    "strict": true
  },
  "exclude": ["node_modules"]
}
